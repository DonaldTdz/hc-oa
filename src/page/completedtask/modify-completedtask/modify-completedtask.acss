button+button {
  margin-top: 32rpx;
}

.row {
  display: flex;
  align-items: center;
  padding: 0 30rpx;
}

.page-section-title {
  font-size: 15px;
}

.row-title {
  flex: 1;
  padding-top: 28rpx;
  padding-bottom: 28rpx;
  font-size: 34rpx;
  color: #000;
}

.row-extra {
  flex-basis: initial;
  font-size: 32rpx;
  color: #888;
}

.row-arrow {
  width: 32rpx;
  height: 32rpx;
  margin-left: 20rpx;
}

.am-list-brief{
  font-size: 0.34rem
}